<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="connection.pool_size">100</property>
		<property name="dialect">org.hibernate.dialect.PostgreSQLDialect</property>
		<property name="current_session_context_class">thread</property>
		<property name="cache.provider_class">org.hibernate.cache.NoCacheProvider</property>
		<property name="hibernate.default_schema">control</property>
        <mapping resource="cl/subtel/model/common/control/Archivo.hbm.xml" />
        <mapping resource="cl/subtel/model/common/control/ArchivosTipos.hbm.xml" />
        <mapping resource="cl/subtel/model/common/control/DetalleError.hbm.xml" />
        <mapping resource="cl/subtel/model/common/control/ErrorCodigos.hbm.xml" />
        <mapping resource="cl/subtel/model/common/control/Estados.hbm.xml" />
        <mapping resource="cl/subtel/model/common/control/Procesos.hbm.xml" />
        <mapping resource="cl/subtel/model/common/control/ProcesosDetalle.hbm.xml" />
        <mapping resource="cl/subtel/model/common/control/TiposProceso.hbm.xml" />
        <mapping resource="cl/subtel/model/common/security/Roles.hbm.xml" />
        <mapping resource="cl/subtel/model/common/security/Usuarios.hbm.xml" />
	</session-factory>
</hibernate-configuration>
